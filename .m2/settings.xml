
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"     xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <profiles>
        <profile>
            <id>gitlab-profile</id>
            <repositories>
                <repository>
                    <id>gitlab-maven</id>
                    <name>java-model-schema-repo</name>
                    <url>https://a6500d02.bal.au.boeing.com/api/v4/projects/34/packages/maven</url>
                </repository>
            </repositories>
        </profile>
    </profiles>
 
    <activeProfiles>
        <activeProfile>myprofile</activeProfile>
    </activeProfiles>
    
    <servers>     
        <server>       
        <id>central</id>       
        <username>${env.MAVEN_REPO_USER}</username>       
        <password>${env.MAVEN_REPO_PASS}</password>     
        </server>   
    </servers>
    <proxies>
        <proxy>
            <id>benproxy</id>
            <active>true</active>
            <protocol>http</protocol>
            <host>www-proxy-au.boeing.com</host>
            <port>31060</port>
        </proxy>
    </proxies>
</settings>

